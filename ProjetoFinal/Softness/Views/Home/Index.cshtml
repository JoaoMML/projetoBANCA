   <!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Cadastro de Pessoas
        <small> - Novo(a) </small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Cadastro</a></li>
        <li class="active">Pessoas</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <!-- Default box -->
    <div class="box">
        <div class="box-header with-border">
            <h3 class="box-title"></h3>

            <div class="box-tools pull-right">
                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                        title="Collapse">
                    <i class="fa fa-minus"></i>
                </button>
            </div>
        </div>
        <form class="form-horizontal">
            <fieldset>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="nome">Nome:</label>
                    <div class="col-md-3">
                        <input id="nome" name="nome" type="text" placeholder="Digite aqui o nome.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="dataNascimento">Data de Nascimento:</label>
                    <div class="col-md-2">
                        <input id="dataNascimento" name="dataNascimento" type="text" placeholder="Dia / Mês / Ano" class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="rg">RG:</label>
                    <div class="col-md-3">
                        <input id="rg" name="rg" type="text" placeholder="Digite aqui o RG.." class="form-control input-md" required="">
                        <span class="help-block">*apenas números*</span>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="cpf">CPF:</label>
                    <div class="col-md-3">
                        <input id="cpf" name="cpf" type="text" placeholder="Digite aqui o CPF.." class="form-control input-md" required="">
                        <span class="help-block">*apenas números*</span>
                    </div>
                </div>

                <!-- Multiple Radios -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="genero">Gênero:</label>
                    <div class="col-md-4">
                        <div class="radio">
                            <label for="genero-0">
                                <input type="radio" name="genero" id="genero-0" value="masculino" checked="checked">
                                Masculino
                            </label>
                        </div>
                        <div class="radio">
                            <label for="genero-1">
                                <input type="radio" name="genero" id="genero-1" value="feminino">
                                Feminino
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Select Basic -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="estadoCivil">Estado Civil:</label>
                    <div class="col-md-2">
                        <select id="estadoCivil" name="estadoCivil" class="form-control">
                            <option value="Solteiro">Solteiro(a)</option>
                            <option value="Casado">Casado(a)</option>
                            <option value="Separado">Separado(a)</option>
                            <option value="Divorciado">Divorciado(a)</option>
                            <option value="Viúvo">Viúvo(a)</option>
                        </select>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="rua">Endereço:</label>
                    <div class="col-md-4">
                        <input id="rua" name="rua" type="text" placeholder="Digite aqui o endereço.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="bairro">Bairro:</label>
                    <div class="col-md-4">
                        <input id="bairro" name="bairro" type="text" placeholder="Digite o Bairro.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="numero">N°:</label>
                    <div class="col-md-2">
                        <input id="numero" name="numero" type="text" placeholder="Digite o numero da casa,apto..." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="complemento">Complemento:</label>
                    <div class="col-md-4">
                        <input id="complemento" name="complemento" type="text" placeholder="Casa,apto,conj,etc.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="cidade">Cidade:</label>
                    <div class="col-md-3">
                        <input id="cidade" name="cidade" type="text" placeholder="Digite o nome da cidade.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Select Basic -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="estado">Estado</label>
                    <div class="col-md-4">
                        <select id="estado" name="estado" class="form-control">
                            <option value="AC">AC</option>
                            <option value="AL">AL</option>
                            <option value="AP">AP</option>
                            <option value="AM">AM</option>
                            <option value="BA">BA</option>
                            <option value="CE">CE</option>
                            <option value="DF">DF</option>
                            <option value="ES">ES</option>
                            <option value="GO">GO</option>
                            <option value="MA">MA</option>
                            <option value="MT">MT</option>
                            <option value="MS">MS</option>
                            <option value="MG">MG</option>
                            <option value="PA">PA</option>
                            <option value="PB">PB</option>
                            <option value="PR">PR</option>
                            <option value="PE">PE</option>
                            <option value="PI">PI</option>
                            <option value="RJ">RJ</option>
                            <option value="RN">RN</option>
                            <option value="RS">RS</option>
                            <option value="RO">RO</option>
                            <option value="RR">RR</option>
                            <option value="SC">SC</option>
                            <option value="SP">SP</option>
                            <option value="SE">SE</option>
                            <option value="TO">TO</option>
                        </select>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="cep">Cep:</label>
                    <div class="col-md-4">
                        <input id="cep" name="cep" type="text" placeholder="Informe o cep.." class="form-control input-md" required="">
                        <span class="help-block">*apenas números*</span>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="email">Email:</label>
                    <div class="col-md-4">
                        <input id="email" name="email" type="text" placeholder="Digite o email.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- File Button -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="fotoCliente">Foto:</label>
                    <div class="col-md-4">
                        <input id="fotoCliente" name="fotoCliente" class="input-file" type="file">
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="fone">Telefone:</label>
                    <div class="col-md-4">
                        <input id="fone" name="fone" type="text" placeholder="Digite o telefone principal.." class="form-control input-md" required="">
                        <span class="help-block">*apenas números*</span>
                    </div>
                </div>

                <!-- Text input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="fone2">Telefone 2:</label>
                    <div class="col-md-4">
                        <input id="fone2" name="fone2" type="text" placeholder="Digite o telefone secundário.." class="form-control input-md">
                        <span class="help-block">*apenas números*</span>
                    </div>
                </div>

                <!-- Password input-->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="senhaSistema">Senha Academia:</label>
                    <div class="col-md-4">
                        <input id="senhaSistema" name="senhaSistema" type="password" placeholder="Digite a senha.." class="form-control input-md" required="">

                    </div>
                </div>

                <!-- Button -->
                <div class="form-group">
                    <label class="col-md-4 control-label" for="cadastrar"></label>
                    <div class="col-md-4">
                        <button id="cadastrar" name="cadastrar" class="btn btn-success">Finalizar</button>
                    </div>
                </div>

            </fieldset>
        </form>

    </div>
    <!-- /.box-body -->
    <div class="box-footer">
        Footer
    </div>
    <!-- /.box-footer-->
    <!-- /.box -->
</section>
